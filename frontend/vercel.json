{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://babcock-publishing-backend.onrender.com/api/$1"
    },
    {
      "source": "/uploads/(.*)",
      "destination": "https://babcock-publishing-backend.onrender.com/uploads/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
