{
  "buildCommand": "echo \"Static frontend build: skipped\"",
  "outputDirectory": "frontend",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://babcock-publishing-backend.onrender.com/api/:match*"
    },
    {
      "source": "/uploads/:match*",
      "destination": "https://babcock-publishing-backend.onrender.com/uploads/:match*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
